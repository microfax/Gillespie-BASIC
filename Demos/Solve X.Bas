100 CLS
110 PRINT 
120 PRINT "Solve for X" 
130 PRINT 
140 PRINT "X + X^2 + X^3 = 42" 
150 PRINT 
160 PRINT "Working ..." 
170 A=TIMER
180 WHILE X+X^2+X^3<42
190   X=X+ 1.00000E-04
200 WEND
210 WHILE X+X^2+X^3>42
220   X=X- 1.00000E-06
230 WEND
240 PRINT  : PRINT  : PRINT  : PRINT  : PRINT 
250 PRINT "Solution: X = " ,ROUND(X,5)
260 PRINT 
270 PRINT "X + X^2 + X^3 = " ;ROUND(X+X^2+X^3,6)
280 PRINT 
290 PRINT "Elapsed time = " ,ROUND(TIMER-A,2),"seconds" 
300 PRINT 
